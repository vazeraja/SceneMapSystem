<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <ui:Template name="SettingsTab" src="project://database/Assets/_TNS/SceneSystem/Editor/UIElements/UXML/SettingsTab.uxml?fileID=9197481963319205126&amp;guid=c140eb113c4bef14d8f622922900c076&amp;type=3#SettingsTab" />
    <ui:Template name="GraphTab" src="project://database/Assets/_TNS/SceneSystem/Editor/UIElements/UXML/GraphTab.uxml?fileID=9197481963319205126&amp;guid=b33b01b0cf923ad45b2100646262e94d&amp;type=3#GraphTab" />
