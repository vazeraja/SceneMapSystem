                                <TNS.SceneSystem.Editor.RibbonButton focusable="true" text="Map Settings" />
                                <TNS.SceneSystem.Editor.RibbonButton focusable="true" text="Graph" />
                                <TNS.SceneSystem.Editor.RibbonButton focusable="true" text="Extra" />
